<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <div class="filter">
        <!-- create button to go back to home route -->
        <a href="/">
          <i class="fas fa-angle-double-left"></i>
          Back to Unfiltered List
        </a>
      </div>
    </div>

    <div class="form-title text-center">
      <span class="text-light">
        The Table is Currently Displaying Jobs with In Progress Status
      </span>
    </div>

