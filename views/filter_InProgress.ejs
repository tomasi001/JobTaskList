          <th>Description</th>
          <th>Location</th>
          <th>Priority</th>
          <th>Status</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <!-- include filter inProgress  page -->
        <%- include("include/_filterInProgress") %>
        <!-- /include filter inProgress  page -->
      </tbody>
    </table>
  </div>
</main>

